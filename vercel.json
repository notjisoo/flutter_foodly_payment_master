{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "PORT": "4888",
    "STRIPE_SECRET_KEY": "@stripe_secret_key",
    "STRIPE_WEBHOOK_SECRET": "@stripe_webhook_secret",
    "MONGODB_URI": "@mongodb_uri",
    "FRONTEND_URL": "@frontend_url",
    "MAIN_BACKEND_URL": "@main_backend_url"
  }
} 